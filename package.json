{
  "name": "express-seed",
  "version": "1.0.0",
  "description": "Seed project for express services. With typescript, jest and watcher for changes.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:watchChanged": " jest --watch",
    "build": "tsc",
    "start": "cross-env NODE_ENV=local node --inspect=5858 -r ts-node/register ./src/index.ts environment=local",
    "start:watch": "nodemon",
    "start:production": "cross-env NODE_ENV=production node dist/index.js"
  },
  "author": "Ti/o",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.7",
    "@types/supertest": "^2.0.7",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "cross-env": "^5.2.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "helmet": "^3.15.0"
  }
}
